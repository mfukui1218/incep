# srcs/requirements/nginx/conf/nginx.conf

worker_processes 1;

events {
    worker_connections 1024;
}

http {
    include       mime.types;
    default_type  application/octet-stream;

    # ここで default.conf を読み込ませる
    include /etc/nginx/conf.d/*.conf;

    sendfile        on;
    keepalive_timeout  65;
}
